{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Total Predictions",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "ticket_triage_predictions_total",
          "legendFormat": "Total"
        }
      ],
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "steps": [{ "color": "green", "value": null }] } }
      }
    },
    {
      "title": "Automation Rate",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "ticket_triage_auto_routed_total / ticket_triage_predictions_total * 100",
          "legendFormat": "Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "max": 100, "min": 0, "unit": "percent",
          "thresholds": { "steps": [
            { "color": "red", "value": null },
            { "color": "yellow", "value": 50 },
            { "color": "green", "value": 75 }
          ]}
        }
      }
    },
    {
      "title": "Average Confidence",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "ticket_triage_confidence_avg",
          "legendFormat": "Confidence"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "max": 1, "min": 0,
          "thresholds": { "steps": [
            { "color": "red", "value": null },
            { "color": "yellow", "value": 0.7 },
            { "color": "green", "value": 0.85 }
          ]}
        }
      }
    },
    {
      "title": "Avg Processing Time (ms)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "ticket_triage_processing_time_ms_avg",
          "legendFormat": "ms"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": { "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 100 },
            { "color": "red", "value": 200 }
          ]}
        }
      }
    },
    {
      "title": "Predictions Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "rate(ticket_triage_predictions_total[5m])",
          "legendFormat": "Predictions/sec"
        }
      ]
    },
    {
      "title": "Action Distribution",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        { "expr": "ticket_triage_auto_routed_total", "legendFormat": "Auto-Routed" },
        { "expr": "ticket_triage_human_reviewed_total", "legendFormat": "Human Review" },
        { "expr": "ticket_triage_escalated_total", "legendFormat": "Escalated" }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["ticket-triage", "ml", "monitoring"],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "title": "Ticket Triage - ML Performance",
  "uid": "ticket-triage-main"
}
